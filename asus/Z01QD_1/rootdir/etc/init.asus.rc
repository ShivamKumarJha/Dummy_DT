#+++ASUS_BSP : add for charger mode to enable uart console , adb and mount asdf, cache folder
on charger
#mount persist
    wait /dev/block/bootdevice/by-name/persist
    exec /system/bin/e2fsck -p /dev/block/bootdevice/by-name/persist
    mkdir /persist/charger
    mount ext4 /dev/block/bootdevice/by-name/persist /persist nosuid nodev noatime barrier=1
    chmod 0777 /persist
    restorecon_recursive /persist
    write /persist/charger/CHGLimit 0

#mount asdf  
    wait /dev/block/bootdevice/by-name/asdf
    exec /system/bin/e2fsck -p /dev/block/bootdevice/by-name/asdf
    mkdir /vendor/asdf
    mount ext4 /dev/block/bootdevice/by-name/asdf /vendor/asdf nosuid nodev noatime barrier=1
    chmod 0777 /vendor/asdf
    restorecon_recursive /vendor/asdf

    write /proc/asusevtlog  "ASDF: Check LastShutdown log."
    write /proc/asusdebug  get_asdf_log
    write /proc/asusevtlog "charger: enter charger mode"

    start logd
    start logcat-charger
#---ASUS_BSP : add for charger mode to enable uart console , adb and mount asdf folder

