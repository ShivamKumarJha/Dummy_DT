on early-init
    write /proc/bootprof "INIT:early-init"
    exec u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules audio_prince audio_madera_mfd audio_madera_pinctrl audio_madera_irq audio_madera_micsupp audio_madera_extcon audio_madera_gpio audio_madera_sound audio_madera_voicetrigger audio_apr audio_wglink audio_adsp_loader audio_q6_pdr audio_q6_notifier audio_q6 audio_usf audio_native audio_pinctrl_wcd audio_pinctrl_lpi audio_swr audio_platform audio_hdmi audio_wcd_spi audio_stub audio_digital_cdc audio_analog_cdc audio_aqt1000_cdc audio_msm_sdw audio_wcd_cpe audio_cpe_lsm audio_wcd9335 audio_machine_sdm710
    write /proc/sys/kernel/sched_boost 1
    mkdir /firmware 0771 system system
    mkdir /bt_firmware 0771 system system
    mkdir /dsp 0771 media media
    symlink /data/tombstones /tombstones

on init
    write /dev/stune/foreground/schedtune.sched_boost_no_override 1
    write /dev/stune/top-app/schedtune.sched_boost_no_override 1
    write /dev/stune/schedtune.colocate 0
    write /dev/stune/background/schedtune.colocate 0
    write /dev/stune/system-background/schedtune.colocate 0
    write /dev/stune/foreground/schedtune.colocate 0
    write /dev/stune/top-app/schedtune.colocate 1
    write /sys/module/qpnp_rtc/parameters/poweron_alarm 1

    mkdir /mnt/cd-rom 0555 system system
    # FLyme:chenjianbo add vibrator permession {@
        chmod 0666 /sys/class/meizu/motor/on_off
        chown system system /sys/class/meizu/motor/on_off
        chmod 0660 /sys/class/meizu/motor/set_cspress
        chown system system /sys/class/meizu/motor/set_cspress
        chmod 0660 /sys/class/meizu/motor/set_mback
        chown system system /sys/class/meizu/motor/set_mback
        chmod 0660 /sys/class/meizu/motor/rtp
        chown system system /sys/class/meizu/motor/rtp
        chmod 0660 /sys/class/meizu/motor/set_rtp
        chown system system /sys/class/meizu/motor/set_rtp
    # @}
    # FLyme:chenjianbo add cs_press  permession {@
        chmod 0666  /sys/cs_press/cs_press/sleep
        chown system system  /sys/cs_press/cs_press/sleep
    # @}
on fs
    write /proc/bootprof "INIT:Mount_START"
    wait /dev/block/platform/soc/${ro.boot.bootdevice}
    symlink /dev/block/platform/soc/${ro.boot.bootdevice} /dev/block/bootdevice
    mount_all /vendor/etc/fstab.qcom
    restorecon_recursive /persist
    mkdir /persist/data 0700 system system

    mount iso9660 loop@/system/etc/iAmCdRom.iso /mnt/cd-rom ro
    write /proc/bootprof "INIT:Mount_END"

on post-fs
    write /proc/bootprof "INIT:post-fs"
    start qseecomd
    exec - system system -- /vendor/bin/init.qti.qseecomd.sh
    write /dev/ipa 1

