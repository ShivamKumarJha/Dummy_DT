    user system
    group system
    oneshot

service mmi-touch-sh /vendor/bin/init.mmi.touch.sh
    class core
    user root
    oneshot

service oem-hw-sh /vendor/bin/init.oem.hw.sh
    class core
    user root
    oneshot
    disabled

service touch-dead-sh /vendor/bin/init.oem.hw.sh -f
