#nubia, tp&lcd node start
#on post-fs-data
on boot
    mkdir /data/vendor/tp 0775 system system
    mkdir /data/vendor/tp2 0775 system system
    mkdir /data/vendor/tp/debug 0775 system system
    rm /data/vendor/tp/ic_ver
    rm /data/vendor/tp/rawdata
    rm /data/vendor/tp/delta
    rm /data/vendor/tp/ic_detect
    rm /data/vendor/tp/start_update
    rm /data/vendor/tp/easy_sleep_palm
    rm /data/vendor/tp/easy_wakeup_gesture
    rm /data/vendor/tp/ic_ver1
    rm /data/vendor/tp/rawdata1
    rm /data/vendor/tp/delta1
    rm /data/vendor/tp/ic_detect1
    rm /data/vendor/tp/start_update1
    rm /data/vendor/tp/easy_sleep_palm1
    rm /data/vendor/tp/easy_wakeup_gesture1
    rm /data/vendor/tp2/ic_ver
    rm /data/vendor/tp2/palm
    rm /data/vendor/tp2/gesture_enable

    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/ic_ver
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/gamemode
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/ic_ver /data/vendor/tp/ic_ver
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/autotest
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/autotest /data/vendor/tp/ic_detect
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/start_update
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/start_update /data/vendor/tp/start_update
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/rawdata
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/rawdata /data/vendor/tp/rawdata
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/delta
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/f54/delta /data/vendor/tp/delta
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/palm_sleep
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/palm_sleep /data/vendor/tp/easy_sleep_palm
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/wake_gesture
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/wake_gesture /data/vendor/tp/easy_wakeup_gesture
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/start_update 1

#add for input6
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/ic_ver
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/gamemode
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/ic_ver /data/vendor/tp/ic_ver1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/autotest
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/autotest /data/vendor/tp/ic_detect1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/start_update
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/start_update /data/vendor/tp/start_update1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/rawdata
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/rawdata /data/vendor/tp/rawdata1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/delta
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/f54/delta /data/vendor/tp/delta1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/palm_sleep
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/palm_sleep /data/vendor/tp/easy_sleep_palm1
    chown system system /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/wake_gesture
    symlink /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/wake_gesture /data/vendor/tp/easy_wakeup_gesture1
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/start_update 1


#add for goodix touch
    chown system system /sys/devices/platform/goodix_ts.0/chip_info
    symlink /sys/devices/platform/goodix_ts.0/chip_info /data/vendor/tp2/ic_ver
    chown system system /sys/devices/platform/goodix_ts.0/palm
    symlink /sys/devices/platform/goodix_ts.0/palm /data/vendor/tp2/palm
    chown system system /sys/devices/platform/goodix_ts.0/gesture/enable
    symlink /sys/devices/platform/goodix_ts.0/gesture/enable /data/vendor/tp2/gesture_enable

#lcd
    chown system system /sys/kernel/lcd_enhance/cabc
	chown system system /sys/kernel/lcd_enhance/dfps

on property:sys.nubia.touch.private=1
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/gamemode 1
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/gamemode 1

on property:sys.nubia.touch.private=0
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input5/gamemode 0
    write /sys/devices/platform/soc/c80000.i2c/i2c-5/5-0020/input/input6/gamemode 0
#nubia end
