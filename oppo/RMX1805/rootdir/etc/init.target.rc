    start cnss_diag

#xuzheng@RH.CN.Wifi.Basic.Log.1928504,2019/05/09,disable feature for collecting wifi driver log
#on property:persist.sys.assert.panic=false
#    stop cnss_diag
#    start cnss_diag_b
#endif

on property:oppo.wifi.ftm=1
