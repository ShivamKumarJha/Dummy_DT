# do not have single modem driver, use modem,adb instead.
on property:sys.usb.config=modem && property:sys.usb.configfs=0
    stop adbd
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 19D2
