service vendor-usb-sh  /system/bin/init.vendor.usb.sh
    class core
    user root
    oneshot

service usbconfig /system/bin/usbconfig
    class main
    user root
    oneshot

# For vts case testExportedPlatformPropertyIntegrity
on boot
    setprop sys.usb.mtp.device_type 3
    setprop sys.usb.mtp.device_type ${sys.usb.mtp.device_type_tablet}

########################################################
# Belows are ProductIDs used by usb.configfs=0
########################################################

# For rmnet transport, convert it here for Emode APK's backwards compatibility.
on property:sys.usb.config=diag,modem,rmnet_bam,adb && property:sys.usb.configfs=0
    setprop sys.usb.config diag,modem,rmnet_ipa,adb

on property:sys.usb.config=diag,modem,rmnet_bam && property:sys.usb.configfs=0
    setprop sys.usb.config diag,modem,rmnet_ipa

on property:sys.usb.config=diag,modem,nmea && property:sys.usb.configfs=0
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 19D2
    write /sys/class/android_usb/android0/idProduct 0111
    write /sys/class/android_usb/android0/f_diag/clients diag
    write /sys/class/android_usb/android0/f_serial/transports smd,tty
    write /sys/class/android_usb/android0/functions diag,serial
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=cdrom,adb && property:sys.usb.configfs=0
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 19D2
    write /sys/class/android_usb/android0/idProduct 0501
    write /sys/class/android_usb/android0/functions mass_storage,adb
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=cdrom && property:sys.usb.configfs=0
